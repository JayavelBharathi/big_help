<div class="row">

  <%= link_to "Add CSR", new_csr_path %>
  <br/>
  <!-- Not implemented on tutorial -->
  <div class="">
    <div style="height: 300px; overflow-y: auto;">
      <div class="panel panel-default">
        <!-- Default panel contents -->
        <div class="panel-heading">Registered Users</div>

        <!-- Table -->
        <table class="table">
          <thead>
          <tr>
            <th>#</th>
            <th>Name</th>
            <th>Email</th>
          </tr>
          </thead>
          <tbody>
          <% @csr_users.each_with_index do |user, index| %>
              <tr>
                <td><%= index + 1 %></td>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
              </tr>
          <% end %>

          </tbody>
        </table>
      </div>

    </div>
  </div>

</div>